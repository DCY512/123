<launch>
    <!-- 启动base.launch文件 -->
    <!-- Launch the base.launch file -->
    <include file="$(find ros_uwb_laser)/launch/base.launch"/>
    
    <!-- 启动激光雷达避障节点 -->
    <!-- Activate lidar obstacle avoidance node -->
    <node name='laser_Avoidance' pkg="ros_uwb_laser" type="laser_Avoidance.py" required="true" output="screen"/>
</launch>
